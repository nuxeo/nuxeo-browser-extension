{

  "manifest_version": 2,
  "name": "Nuxeo Dev Tools",
  "version": "0.0.1",

  "description": "",

  "icons": {
    "48": "images/nuxeo-48.png",
    "96": "images/nuxeo-96.png"
  },
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "devtools@nuxeo.com",
      "strict_min_version": "45.0",
      "strict_max_version": "51.*"
    }
  },
  "background": {
    "scripts": [
			"scripts/background.js"]
  },
  "page_action": {
    "default_icon": {
      "16": "images/nuxeo-16.png",
      "19": "images/nuxeo-19.png",
      "32": "images/nuxeo-32.png",
      "38": "images/nuxeo-38.png"
    },
    "default_title": "Nuxeo FF Dev Tools",
    "default_popup": "popup.html"
  },
  "permissions": [
		"tabs",
		"http://*/*",
		"https://*/*",
		"activeTab",
		"https://ajax.googleapis.com/",
		"notifications"
  ]

}
