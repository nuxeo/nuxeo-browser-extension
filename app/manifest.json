{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/nuxeo-16.png",
    "128": "images/nuxeo-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "https://ajax.googleapis.com/",
    "notifications",
    "background",
    "declarativeContent"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "omnibox": {
    "keyword": "OMNIBOX-KEYWORD"
  },
  "page_action": {
    "default_icon": {
      "19": "images/nuxeo-19.png",
      "38": "images/nuxeo-38.png"
    },
    "default_title": "Nuxeo Extension",
    "default_popup": "popup.html"
  }
}
